<div id="footer_flex">
  <div class="links">
    <a href="/index.html">Home</a>
    <a href="/talent.html">Talent</a>
    <a href="/whats_new.html">What's New?</a>
    <a href="/about2.html">About</a>
    <a href="/contact.html">Contact</a>
  </div>
  <div class="socials">
    <img class="ikon" src="/imgs/instalogo.svg" alt="insta" />
    <img class="ikon" src="/imgs/facebooklogo.svg" alt="facebook" />
  </div>
</div>
<style>
  @import url("https://fonts.googleapis.com/css2?family=Work+Sans:wght@600&display=swap");
  img {
    width: 20px;
    height: auto;
    transition: 1s;
    cursor: pointer;
  }
  #footer_flex {
    justify-content: space-between;
    align-items: center;
    z-index: 10;
    display: flex;
    padding-left: 20px;
    padding-right: 20px;
  }

  .links {
    gap: 10px;
    display: flex;
  }
  .ikon {
    position: static;
  }
  .socials {
    gap: 5px;
    display: flex;
  }
  a {
    font-family: "Work Sans";
    font-size: 12px;
    font-weight: 600;
    text-decoration: none;
    color: #4a4a4a;
  }
  a:hover {
    text-decoration: underline;
  }
  .ikon:hover {
    scale: 1.05;
  }
  @media (min-width: 800px) {
    a {
      font-size: 15px;
    }
    .links {
      gap: 30px;
    }
    .socials {
      gap: 20px;
    }
    img {
      width: 30px;
    }
  }
</style>
